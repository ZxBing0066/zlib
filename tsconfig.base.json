{
    "compilerOptions": {
        "noImplicitAny": true,
        "emitDeclarationOnly": true,
        "declaration": true,
        "module": "commonjs",
        "target": "es5",
        "allowJs": false,
        "listEmittedFiles": true,
        "charset": "utf-8",
        "strictNullChecks": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "include": [],
    "exclude": ["node_modules", "**/node_modules"]
}
